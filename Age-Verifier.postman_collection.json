		{
			"name": "postUserAge_Number above 0 to 18",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let userIsAllowedToDrink = pm.environment.get(\"responseIsTrueOrFalse\");",
							"",
							"var responseData = JSON.parse(responseBody);",
							"",
							"tests[\"Validate status code\"] = responseCode.code === 200;",
							"for(let key of Object.keys(responseData)){",
							"  var expectedKey= key;",
							"};",
							"var schema = {",
							"    \"type\": \"object\",",
							"    \"properties\": {",
							"        \"isValid\": {\"type\":\"boolean\"}",
							"    },",
							"    \"required\" : [\"isValid\"]",
							"};",
							"",
							"pm.test('Schema is valid', function () {",
							"    pm.response.to.have.jsonSchema(schema)",
							"});",
							"tests[\"Validate JSON has keys \"] =  expectedKey === \"isValid\";",
							"tests[\"Validate user is allowed to drink if the age is 12\"] = responseData[expectedKey] === userIsAllowedToDrink;",
							"tests[\"Validate the header is of JSON\"] = postman.getResponseHeader(\"Content-Type\") == \"application/json; charset=utf-8\";",
							"",
							"",
							"",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"let max = 17",
							"let min = 0",
							"",
							"let userIsAllowedToDrink = pm.environment.get(\"responseIsTrueOrFalse\");",
							"let userAge = Math.floor(Math.random() * (max - min + 1) + min);",
							"console.log(userAge);",
							"pm.environment.set(\"responseIsTrueOrFalse\", false);",
							"console.log(pm.environment.get(\"responseIsTrueOrFalse\"));",
							"pm.environment.set(\"userAge\", userAge);",
							"",
							"",
							"",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
